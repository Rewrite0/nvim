{
  "import": {
    "scope": "javascript,typescript",
    "prefix": "imp",
    "body": ["import ${0:module} from '${1:package}';"],
    "description": "Import a module"
  },
  "export-all": {
    "scope": "javascript,typescript",
    "prefix": "ex",
    "body": ["export * from '$2';"],
    "description": "Export a module"
  }
}
